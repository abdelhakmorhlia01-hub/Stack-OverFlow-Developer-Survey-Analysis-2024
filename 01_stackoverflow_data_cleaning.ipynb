     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# List of bins for grouping years of professional experience\n",
    "bins=[0,5,10,20,30,40,49,50]\n",
    "\n",
    "# Create corresponding labels for each experience range\n",
    "labels=['0-5 years','6-10 years','11-20 years','21-30 years','31-40 years','41-49 years','+50 years']\n",
