    "# Categorize YearsCodePro_Normalized into experience groups\n",
    "# and create a new column called 'ProExperience'\n",
    "df['ProExperience']=pd.cut(df['YearsCodePro_Normalized'],bins=bins,labels=labels)\n",
    "\n",
    "# Display unique values of ProExperience\n",
    "df['ProExperience'].unique()"
